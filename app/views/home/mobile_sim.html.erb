<pre>{"state": {
	"connectedToServer": "true", 
	"availableToSpend": "<%= current_user.home_asset_type.to_currency(current_user.available_to_spend) %>", 
	"fiDate": "<%= current_user.fi_date.to_formatted_s(:long_ordinal) %>",
	"recentTransactions": [
		<%= current_user.recent_transactions.map { |t| render t }.join(", ").html_safe %>
	],
	"accounts": {
		"expense": {
			"list": [
			<%= current_user.frequent_expense_accounts.map { |a| render a }.join(", ").html_safe %>
		]},
		"spending": {
			"list": [
			<%= current_user.frequent_spending_accounts.map { |a| render a }.join(", ").html_safe %>
		]}
	},
	"budgetGoals": {
		"list": [
		<%= render BudgetGoal.new %>,
		<%= current_user.budget_goals.map { |bg| render bg }.join(", ").html_safe %>
	]}
}}</pre>