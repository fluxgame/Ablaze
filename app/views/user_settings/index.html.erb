<p id="notice"><%= notice %></p>

<h1>User Settings</h1>

<table>
  <thead>
    <tr>
      <th>Home asset type</th>
      <th>Withdrawal rate</th>
      <th>Goal savings rate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_settings.each do |user_setting| %>
      <tr>
        <td><%= user_setting.home_asset_type %></td>
        <td><%= user_setting.withdrawal_rate %></td>
        <td><%= user_setting.goal_savings_rate %></td>
        <td><%= link_to 'Show', user_setting %></td>
        <td><%= link_to 'Edit', edit_user_setting_path(user_setting) %></td>
        <td><%= link_to 'Destroy', user_setting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Setting', new_user_setting_path %>
