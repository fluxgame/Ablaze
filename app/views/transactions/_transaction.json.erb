{
			"id": "<%= transaction.id.to_s %>",
			"date": "<%= transaction.master_ledger_entry.date.to_s %>",
			"description": "<%= transaction.description %>",
			"masterLedgerEntry": <%= render local_assigns[:master_ledger_entry] %>,
			"slaveLedgerEntries": [
				<%= transaction.ledger_entries.map { |le| render le if le.id != local_assigns[:master_ledger_entry].id }.join(", ").html_safe %>
			]
}